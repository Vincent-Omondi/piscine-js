function first(input){
    return input[0];
}

function last(input){
    return input[input.length -1];
}

function kiss(input){
    return [last(input), first(input)];
}

// console.log(first("hello")); // Output: "h"
// console.log(last([1, 2, 3])); // Output: 3
// console.log(kiss([1, 2, 3])); // Output: [3, 1]
